{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "lib": ["ES2022"],
    "types": ["node"],
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "lib",
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  //"exclude": ["node_modules", "../**/*", "**/*.test.ts"]
}
